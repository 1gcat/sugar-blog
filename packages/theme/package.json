{
  "name": "@sugarat/theme",
  "version": "0.0.0",
  "description": "粥里有勺糖的博客主题，sugarat vitepress blog theme",
  "main": "src/index.ts",
  "exports": {
    "./node": "./node.js",
    ".": "./src/index.ts"
  },
  "files": [
    "src",
    "types",
    "node.js",
    "node.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATQQ/sugar-blog.git"
  },
  "keywords": [
    "vitepress",
    "theme",
    "粥里有勺糖"
  ],
  "author": "sugar",
  "license": "MIT",
  "homepage": "https://next.sugarat.top",
  "bugs": {
    "url": "https://github.com/ATQQ/sugar-blog/issues"
  },
  "scripts": {
    "dev": "npm run build:node && vitepress dev demo",
    "build:demo": "vitepress build demo",
    "build:node":"npx tsup src/node.ts --dts --out-dir=./",
    "serve": "npm run build:node && npm run build:demo && vitepress serve demo"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.6.0",
    "element-plus": "^2.2.26",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "typescript": "^4.8.2",
    "vitepress": "^1.0.0-alpha.33",
    "vue": "^3.2.45",
    "sass": "^1.56.1",
    "tsup":" ^6.5.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
